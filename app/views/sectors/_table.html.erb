<table id="sectors">
	<thead>
		<tr>
			<th>Received (USD)</th>
			<th>Projects</th>
			<th>Name</th>
    	<th>Category</th>
    	<th></th>
		</tr>
  </thead>
</table>

<script>
$(document).ready(function() {
	$('#sectors').dataTable({
		"bProcessing": true,
		"sAjaxSource": "<%= request.url %>.json",
		"sAjaxDataProp": "sectors",
		"aoColumns": [
			{"mDataProp":"received", "sType":"currency", "sWidth":"100px", "asSorting":["desc","asc"] },
			{"sWidth":"50px", "mDataProp":"projects"},
			{"fnRender":function(oObj) { return '<a href="'+oObj.aData['sector_url']+'">'+oObj.aData['name']+'</a>'; }},
			{"mDataProp":"category"},
		],
		"sPaginationType": "full_numbers"
	});
} );
</script>