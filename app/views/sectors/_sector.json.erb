<% cache("sectors/#{sector.id}-#{sector.updated_at}/#{start_year}/#{end_year}/show") do %>{
		"name": "<%= sector.name %>",
		"category": "<%= sector.category %>",
		"projects": <%= sector.projects.size %>,
		"url": "<%= url_for sector %>",
		"valid": <%= sector.valid? %>,

		"received": "<%= sector.received %>",
		"live_received": <%= sector.received %>,

		"icon": "<%= sector.icon %>",
		"sector_id": <%= sector.id %>
}<% end %>