<% cache sector do %>{
	"name": "<%= sector.name %>", 
	"category": "<%= sector.category %>", 
	"url": "<%= url_for sector %>",
	
	"received": {<% (START_YEAR..END_YEAR).each do |y| %>
		"<%= y %>": <%= sector.received(Date.civil(y.to_i,1,1),Date.civil(y.to_i,12,31),sector.live_projects) %>,<% end %>
		"all":  <%= sector.received(Date.civil(START_YEAR.to_i,1,1),Date.civil(END_YEAR.to_i,12,31),sector.live_projects) %>
	},

	"icon": "<%= sector.icon %>",
	"sector_id": <%= sector.id %>
}<% end %>