<% cache ['admin', sector] do %>{
	"name": "<%= sector.name %>",
	"category": "<%= sector.category %>",
	"url": "<%= url_for sector %>",
	"valid": <%= sector.valid? %>,
	"projects": <%= sector.projects.size %>,
	"received": <%= sector.received %>,
	"icon": "<%= sector.icon %>",
	"sector_id": <%= sector.id %>
}<% end %>