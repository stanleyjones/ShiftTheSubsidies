{
	"sectors":[
	<% # List all @sectors %>
	<% @sectors.each do |sector| %>{
		"valid":"<%= sector.valid? %>",
		"received":"<%= number_to_currency( sector.received ) %>",
		"amount_raw": <%= sector.received %>,
		"projects":<%= sector.projects.size %>,
		"name": "<%= sector.name %>",
		"category": "<%= sector.category %>",
		"sector_url": "<%= url_for [:admin,sector] %>"
	}<% unless sector == @sectors.last %>,<% end %>
	<% end %>
	]
}