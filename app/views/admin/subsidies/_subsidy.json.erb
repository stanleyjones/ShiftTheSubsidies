<% cache ['admin',subsidy] do %>{
		"amount": <%= subsidy.amount %>,
		"valid": <%= subsidy.valid? %>,
		"subsidy_id": <%= subsidy.id %>,
		"date": "<%= subsidy.date %>",
		"from": "<% if subsidy.institution %><%= subsidy.institution.name %><% end %>",
		"institution_id": "<%= subsidy.institution_id %>",
		"to": "<% if subsidy.entity %><%= subsidy.entity.name %><% end %>",
		"entity_id": "<%= subsidy.entity_id %>",
		"for": "<% if subsidy.project %><%= subsidy.project.name %><% end %>",
		"sector": "<% if subsidy.sector %><%= subsidy.sector.name %><% end %>",
		"kind": "<%= subsidy.kind %>",
		"project_id": "<%= subsidy.project_id %>"
	}<% end %>