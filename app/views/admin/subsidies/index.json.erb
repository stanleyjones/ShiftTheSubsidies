{
	"subsidies":[
	<% # List all @subsidies %>
	<% @subsidies.each do |subsidy| %><% cache(subsidy) do %>{
		"valid": "<%= subsidy.valid? %>",
		"amount": "<%= number_to_currency( subsidy.amount, :precision => 0 ) %>",
		"amount_raw": <%= subsidy.amount %>,
		"date": "<%= subsidy.date %>",
		"from": "<% if subsidy.institution %><%= subsidy.institution.name %><% end %>",
		"to": "<% if subsidy.entity %><%= subsidy.entity.name %><% end %>",
		"for": "<% if subsidy.project %><%= subsidy.project.name %><% end %>",
		"institution_url": "/admin/institutions/<%= subsidy.institution_id %>",
		"entity_url": "/admin/entities/<%= subsidy.entity_id %>",
		"project_url": "/admin/projects/<%= subsidy.project_id %>",
		"subsidy_url": "/admin/subsidies/<%= subsidy.id %>"
	}<% end %><% unless subsidy == @subsidies.last %>,<% end %>
	<% end %>
	]
}