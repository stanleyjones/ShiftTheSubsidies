<% cache("institutions/#{institution.id}-#{institution.updated_at}/#{start_year}/#{end_year}/show") do %>{
		"name": "<%= institution.name %>",
		"abbreviation": "<%= institution.abbreviation %>",
		"description": "<%= institution.description %>",
		"url": "<%= url_for institution %>",
		"valid": <%= institution.valid? %>,

		"total": <%= institution.awarded(@start_date,@end_date) %>,
		"clean": <%= institution.awarded_to_category('Clean',@start_date,@end_date) %>,
		"fossil": <%= institution.awarded_to_category('Fossil Fuel',@start_date,@end_date) %>,
		"access": <%= institution.awarded_to_energy_access(@start_date,@end_date) %>,

		"live_total": <%= institution.awarded(@start_date,@end_date,institution.live_subsidies) %>,
		"live_clean": <%= institution.awarded_to_category('Clean',@start_date,@end_date,institution.live_subsidies) %>,
		"live_fossil": <%= institution.awarded_to_category('Fossil Fuel',@start_date,@end_date,institution.live_subsidies) %>,
		"live_access": <%= institution.awarded_to_energy_access(@start_date,@end_date,institution.live_subsidies) %>,

		"institution_id":<%= institution.id %>
	}<% end %>