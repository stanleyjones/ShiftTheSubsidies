		{
			"name": "<%= institution_group.name %>",
			"abbreviation": "<%= institution_group.name %>",
			"url": "<%= url_for institution_group %>",
			"awarded": {<% (START_YEAR..END_YEAR).each do |y| %>
				"<%= y %>": {
					"clean": <%= institution_group.awarded_to_category('Clean',Date.civil(y.to_i,1,1),Date.civil(y.to_i,12,31),institution_group.live_institutions) %>, 
					"fossil": <%= institution_group.awarded_to_category('Fossil Fuel',Date.civil(y.to_i,1,1),Date.civil(y.to_i,12,31),institution_group.live_institutions) %>, 
					"access": <%= institution_group.awarded_to_energy_access(Date.civil(y.to_i,1,1),Date.civil(y.to_i,12,31),institution_group.live_institutions) %>, 
					"total": <%= institution_group.awarded(Date.civil(y.to_i,1,1),Date.civil(y.to_i,12,31),institution_group.live_institutions) %>
				},<% end %>
				"all":  {
					"clean": <%= institution_group.awarded_to_category('Clean',Date.civil(START_YEAR.to_i,1,1),Date.civil(END_YEAR.to_i,12,31),institution_group.live_institutions) %>, 
					"fossil": <%= institution_group.awarded_to_category('Fossil Fuel',Date.civil(START_YEAR.to_i,1,1),Date.civil(END_YEAR.to_i,12,31),institution_group.live_institutions) %>, 
					"access": <%= institution_group.awarded_to_energy_access(Date.civil(START_YEAR.to_i,1,1),Date.civil(END_YEAR.to_i,12,31),institution_group.live_institutions) %>, 
					"total": <%= institution_group.awarded(Date.civil(START_YEAR.to_i,1,1),Date.civil(END_YEAR.to_i,12,31),institution_group.live_institutions) %>
				}
			},
			"institution_group_id":<%= institution_group.id %>
		}