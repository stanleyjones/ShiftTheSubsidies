<% cache("projects/#{project.id}-#{project.updated_at}/#{start_year}/#{end_year}/json") do %>{
		"name": "<%= project.name %>",
		"sector": "<%= project.sector_name %>",
		"category": "<%= project.category %>",
		"icon": "<%= project.icon %>",
		"region": "<%= project.region %>",
		"cc": "<%= project.country_code %>",
		"url": "<%= url_for project %>",
		"valid": <%= project.valid? %>,

		"clean?": <%= project.clean? %>,
		"access?": <%= project.access? %>,

		"received": <%= project.received %>,
		"live_received": <%= project.received(@start_date,@end_date,project.live_subsidies) %>,

		"project_id": <%= project.id %>
}<% end %>